syntax toit "\.toit$"

# Function
color 

# Keyword
color brightwhite "(class|if|else|for|import|as|show)"
color cyan "(true|false|none|null)"

# Meta
color normal "::=" ":\=" ":" ","

# String: double-quoted
color red ""

# String: single-quoted
color red ""
